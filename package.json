{
  "name": "catnip",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "PORT=${PORT:-5173} sh -c 'VITE_PORT=$PORT MOCK_PORT=$((PORT + 1)) concurrently \"MOCK_PORT=$((PORT + 1)) pnpm run mock:server\" \"VITE_USE_MOCK=true VITE_PORT=$PORT MOCK_PORT=$((PORT + 1)) vite\"'",
    "dev:with-mock": "VITE_USE_MOCK=true vite",
    "mock:server": "node mock-server.js",
    "dev:cf": "pnpm run build && wrangler dev 2>&1 | tee /tmp/wrangler.log",
    "dev:cf:catnip": "./container/bin/catnip logs pnpm run build && ./container/bin/catnip logs wrangler dev",
    "dev:cf:vite": "CLOUDFLARE_DEV=true vite --port 8787",
    "build": "tsc -b && vite build",
    "build:fast": "vite build",
    "typecheck": "tsc --noEmit && tsc --noEmit -p tsconfig.worker.json",
    "typecheck:worker": "tsc --noEmit -p tsconfig.worker.json",
    "lint": "eslint .",
    "format": "echo \"Global pretty disabled for now...\" #prettier --write .",
    "format:check": "prettier --check .",
    "format:changed": "{ git diff --cached --name-only --diff-filter=ACMR; git diff --name-only --diff-filter=ACMR; git ls-files --others --exclude-standard; } | grep -E '\\.(ts|tsx|js|jsx|json|css|md)$' | xargs -r prettier --write",
    "preview": "pnpm run build && vite preview",
    "deploy:prod": "pnpm run build && wrangler deploy --env production",
    "cf-typegen": "wrangler types",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
