    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "prettier": "^3.6.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "@playwright/test": "^1.47.2"
