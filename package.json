{
  "name": "catnip",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:cf": "pnpm run build && wrangler dev 2>&1 | tee /tmp/wrangler.log",
    "dev:cf:catnip": "./container/bin/catnip logs pnpm run build && ./container/bin/catnip logs wrangler dev",
    "dev:cf:vite": "CLOUDFLARE_DEV=true vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc --noEmit && tsc --noEmit -p tsconfig.worker.json",
    "typecheck:worker": "tsc --noEmit -p tsconfig.worker.json",
    "lint": "eslint .",
    "format": "echo \"Global pretty disabled for now...\" #prettier --write .",
    "format:check": "prettier --check .",
    "format:changed": "{ git diff --cached --name-only --diff-filter=ACMR; git diff --name-only --diff-filter=ACMR; git ls-files --others --exclude-standard; } | grep -E '\\.(ts|tsx|js|jsx|json|css|md)$' | xargs -r prettier --write",
    "preview": "pnpm run build && vite preview",
    "test:e2e": "playwright test",
    "test:e2e:headed": "PLAYWRIGHT_HEADLESS=0 playwright test -g workspace",
    "deploy:prod": "pnpm run build && wrangler deploy --env production",
    "cf-typegen": "wrangler types",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@cloudflare/containers": "^0.0.22",
    "@hono/oauth-providers": "^0.8.2",
    "@octokit/app": "^16.0.1",
    "@octokit/core": "^7.0.3",
    "@octokit/webhooks": "^14.1.3",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-router": "^1.130.12",
    "@tanstack/react-router-devtools": "^1.130.12",
    "@types/prismjs": "^1.26.5",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "hono": "^4.8.10",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "prismjs": "^1.30.0",
    "react": "^19.1.1",
    "react-day-picker": "^9.8.1",
    "react-diff-viewer-continued": "^3.4.0",
    "react-dom": "^19.1.1",
    "react-hotkeys-hook": "^5.1.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.4",
    "react-xtermjs": "^1.0.10",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.11.0",
    "@cloudflare/workers-types": "^4.20250801.0",
    "@eslint/js": "^9.32.0",
    "@inquirer/prompts": "^7.8.0",
    "@tanstack/router-plugin": "^1.130.12",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^3.11.0",
