<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Allow JIT compilation for performance -->
    <key>com.apple.security.cs.allow-jit</key>
    <true/>
    
    <!-- Allow unsigned executable memory for Go runtime -->
    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>
    
    <!-- Allow execution of dynamically signed code -->
    <key>com.apple.security.cs.allow-dyld-environment-variables</key>
    <true/>
    
    <!-- Hardened runtime -->
    <key>com.apple.security.cs.disable-library-validation</key>
    <false/>
    
    <!-- Network access for CLI tool -->
    <key>com.apple.security.network.client</key>
    <true/>
    
    <!-- Allow outbound network connections -->
    <key>com.apple.security.network.server</key>
    <true/>
    
    <!-- Note: NSUserNotification doesn't require special entitlements -->
    <!-- This communication entitlement is for advanced features we don't use -->
    <!-- Keeping it doesn't hurt but isn't necessary for basic notifications -->
    <key>com.apple.developer.usernotifications.communication</key>
    <true/>
</dict>
</plist>