{
  "name": "catnip-sidebar",
  "displayName": "W&B Catnip",
  "description": "W&B Catnip - a highly addictive agentic coding environment",
  "version": "1.0.0",
  "publisher": "wandb",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "media/catnip.png",
  "repository": "https://github.com/wandb/catnip",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "catnip": [
        {
          "id": "catnip-sidebar",
          "name": "Catnip"
        }
      ]
    },
    "commands": [
      {
        "command": "catnip.openInterface",
        "title": "Open Catnip Interface",
        "icon": "üêæ"
      },
      {
        "command": "catnip.openLogs",
        "title": "Open Catnip Logs",
        "icon": "üìã"
      },
      {
        "command": "catnip.showQR",
        "title": "Show QR Code",
        "icon": "üì±"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "catnip",
          "title": "Catnip",
          "icon": "media/catnip.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "catnip.openInterface",
          "when": "view == catnip-sidebar",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "npx @vscode/vsce package --out ../catnip-sidebar.vsix",
    "build": "npm run compile && npm run package"
  },
  "dependencies": {
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "^18.0.0",
    "@types/qrcode": "^1.5.5",
    "typescript": "^5.0.0",
    "@vscode/vsce": "^2.21.0"
  }
}
